    <validationRules>
        <fullName>Validate_Endpoint_URL</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!BEGINS(API_Endpoint__c, &quot;http://&quot;),
    !BEGINS(API_Endpoint__c, &quot;https://&quot;))</errorConditionFormula>
        <errorDisplayField>API_Endpoint__c</errorDisplayField>
        <errorMessage>Endpoint must start with http:// or https://</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Endpoint_URL_Ends_With_v2_soap</fullName>
        <active>true</active>
        <errorConditionFormula>!CONTAINS(RIGHT(API_Endpoint__c, 7), &quot;v2_soap&quot;)</errorConditionFormula>
        <errorDisplayField>API_Endpoint__c</errorDisplayField>
        <errorMessage>Endpoint must end with /api/v2_soap</errorMessage>
    </validationRules>
