        <url>var ids= {!GETRECORDIDS($ObjectType.FBA_Shipment_Item__c)}; if(ids.length &lt; 1 ) alert(&apos;Please select at least one Order to send to Amazon.&apos;); else if(ids.length &gt; 10 ) alert(&apos;Sorry, you can only select 10 orders.&apos;); else var mywin = window.open(&apos;/apex/eCommazon__CreateInboundShipmentPlan?ids=&apos;+ids.join(&apos;,&apos;));</url>
