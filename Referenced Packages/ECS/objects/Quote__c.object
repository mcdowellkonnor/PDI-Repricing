        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/20.0/connection.js&quot;)} 
{!requireScript(&quot;/soap/ajax/20.0/apex.js&quot;)} 

var qr = sforce.connection.query(&quot;select id from emailtemplate where DeveloperName=&apos;Email_Quote2&apos; AND NamespacePrefix=&apos;ECS&apos;&quot;); 

if(qr.size &lt; 1 ) { 
alert(&apos;Please contact your SF Administrator to create the Email Quote Template&apos;); 
} else { 
var templatedId = qr.getArray(&quot;records&quot;)[0].Id; 
location.replace(&apos;/email/author/emailauthor.jsp?retURL=/{!Quote__c.Id}&amp;p3_lkid={!Quote__c.Id}&amp;rtype=003&amp;p2_lkid={!Quote__c.ContactId__c}&amp;template_id=&apos; + templatedId + &apos;&amp;new_template=1&apos;); 
}</url>
