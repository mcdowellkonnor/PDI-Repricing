    <fields>
        <fullName>Tracking_URL__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>HYPERLINK( 
IF( ISPICKVAL(Shipping_Carrier__c, &quot;US Postal Service&quot;), &quot;https://tools.usps.com/go/TrackConfirmAction?qtc_tLabels1=&quot;, 
IF( ISPICKVAL( Shipping_Carrier__c, &quot;UPS Mail Innovations&quot;), &quot;http://www.ups-mi.net/packageID/default.aspx#&quot;,
IF( ISPICKVAL( Shipping_Carrier__c, &quot;DHL&quot;), &quot;http://webtrack.dhlglobalmail.com/?trackingnumber=&quot;, 
IF( ISPICKVAL( Shipping_Carrier__c, &quot;Royal Mail&quot;), &quot;http://www.royalmail.com/portal/rm/track?trackNumber=&quot;, 
IF( ISPICKVAL( Shipping_Carrier__c, &quot;Parcelforce&quot;), &quot;http://www.parcelforce.com/portal/pw/track?trackNumber=&quot;, 
IF( ISPICKVAL( Shipping_Carrier__c, &quot;Australia Post&quot;), &quot;http://auspost.com.au/track&quot;, 
IF( CONTAINS(LOWER(TEXT(Shipping_Carrier__c)), &quot;fedex&quot;), &quot;http://www.fedex.com/Tracking?action=track&amp;tracknumbers=&quot;,
IF( CONTAINS(LOWER(TEXT(Shipping_Carrier__c)), &quot;ups&quot;), &quot;http://wwwapps.ups.com/WebTracking/processRequest?tracknum=&quot;, 
IF( CONTAINS(LOWER(TEXT(Shipping_Carrier__c)), &quot;usps&quot;), &quot;https://tools.usps.com/go/TrackConfirmAction?qtc_tLabels1=&quot;, 
IF(BEGINS(Tracking_Number__c, &quot;1Z&quot;), &quot;http://wwwapps.ups.com/WebTracking/processRequest?tracknum=&quot;, 
IF(BEGINS(Tracking_Number__c, &quot;LN&quot;), &quot;https://tools.usps.com/go/TrackConfirmAction?qtc_tLabels1=&quot;, 
IF(BEGINS(Tracking_Number__c, &quot;E&quot;), &quot;https://tools.usps.com/go/TrackConfirmAction?qtc_tLabels1=&quot;, 
IF(BEGINS(Tracking_Number__c, &quot;CJ&quot;), &quot;https://tools.usps.com/go/TrackConfirmAction?qtc_tLabels1=&quot;, 
