    <webLinks>
        <fullName>Create_Email_Service</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Email Service</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/25.0/apex.js&quot;)}
var apexResult = sforce.connection.query(&quot;Select Id From ApexClass Where Name=&apos;ProcessECommerceEmail&apos; &quot;);
var records = apexResult.getArray(&quot;records&quot;);
alert(records[0].Id);
var e = new sforce.SObject(&apos;EmailServicesFunction&apos;); 
        e.OverLimitAction = &apos;3&apos;;
        e.IsTlsRequired = false;
        e.IsTextAttachmentsAsBinary = false;
        e.IsErrorRoutingEnabled = true;
        e.IsAuthenticationRequired = false;
        e.IsActive = true;
        e.FunctionName = &apos;UPS&apos;;
        e.FunctionInactiveAction = &apos;2&apos;;
        e.ErrorRoutingAddress = &apos;jon@cloudconversion.com&apos;;
        e.AuthorizationFailureAction = &apos;2&apos;;
        e.AuthenticationFailureAction = &apos;2&apos;;
        e.AttachmentOption = &apos;3&apos;;
        e.ApexClassId = records[0].Id;
        e.AddressInactiveAction = &apos;2&apos;;
var result = sforce.connection.create([e]); 
alert(result.length);
for (var i=0; i&lt;result.length; i++) {
    if(result[i].getBoolean(&quot;success&quot;)) {
       alert(&quot;new email service function created with id &quot; + result[i].id);
        var es = new sforce.SObject(&apos;EmailServicesAddress&apos;); 
        es.RunAsUserId = &apos;{!User.Id}&apos;;
        es.LocalPart = &apos;ups&apos;;
        es.IsActive = true;
        es.FunctionId = result[i].id;
         var result2 = sforce.connection.create([es]); 
         for(var j=0; j&lt;result2.length; j++) {
               if(result2[j].getBoolean(&quot;success&quot;)) {
                   alert(&quot;new email service address created with id &quot; + result2[j].id);
               } else {
                   alert(&quot;failed to create account &quot; + result2[j]);
              }
         }
    } else {
      alert(&quot;failed to create account &quot; + result[i]);
    }
}
window.location.reload();</url>
