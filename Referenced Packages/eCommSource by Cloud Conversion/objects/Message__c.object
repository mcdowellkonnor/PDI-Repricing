        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

var newRecords = [];

var ids= {!GETRECORDIDS($ObjectType.Message__c)};

//Confirming users action
var r = confirm(&apos;Close these messages?&apos;)
if (r == true) {
  if(ids.length &lt; 1 ) {
    alert(&apos;Please select at least one message.&apos;);
  } else {
    for (var n=0; n&lt;ids.length; n++) {
        var sv = new sforce.SObject(&apos;CloudConversion__Message__c&apos;);
        sv.CloudConversion__Status__c = &apos;Closed&apos;;
        sv.OwnerId = &apos;{!User.Id}&apos;;
        sv.id = ids[n];
        newRecords.push(sv);
     }
     result = sforce.connection.update(newRecords);
  }
}
window.location.reload();</url>
